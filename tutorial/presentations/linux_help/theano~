####################################################### 
# how to add gpu to theano
1-  creat  file: 
$sudo gedit $HOME/.theanorc

# 2- add the following to ~/.theanorc
[global]
floatX = float32
device = gpu0

# add a [cuda] section to your .theanorc file containing the option root = /path/to/cuda/root.
[cuda]
root = /usr/local/cuda

